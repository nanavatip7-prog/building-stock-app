<hr data-start="2363" data-end="2366">
<h2 data-start="2368" data-end="2406"> STEP 2.1: ADD MONTHLY REPORT API</h2>
<p data-start="2408" data-end="2444"> Paste <strong data-start="2417" data-end="2443">below daily report API</strong>:</p>
<pre class="overflow-visible! px-0!" data-start="2446" data-end="3501"><div class="contain-inline-size rounded-2xl corner-superellipse/1.1 relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary select-none rounded-t-2xl corner-t-superellipse/1.1">js</div><div class="sticky top-[calc(--spacing(9)+var(--header-height))] @w-xl/main:top-9"><div class="absolute end-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-bg-elevated-secondary text-token-text-secondary flex items-center gap-4 rounded-sm px-2 font-sans text-xs"><button class="flex gap-1 items-center select-none py-1" aria-label="Copy"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" aria-hidden="true" class="icon-sm"><use href="/cdn/assets/sprites-core-k5zux585.svg#ce3544" fill="currentColor"></use></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-js"><span><span><span class="hljs-comment">//  Monthly Report</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">"/report/monthly"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> month = req.</span><span><span class="hljs-property">query</span></span><span>.</span><span><span class="hljs-property">month</span></span><span>; </span><span><span class="hljs-comment">// format: YYYY-MM</span></span><span>

  </span><span><span class="hljs-keyword">if</span></span><span> (!month) {
    </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">400</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">"Month required (YYYY-MM)"</span></span><span> });
  }

  </span><span><span class="hljs-keyword">const</span></span><span> report = {};

  </span><span><span class="hljs-comment">// Monthly purchases</span></span><span>
  db.</span><span><span class="hljs-title function_">query</span></span><span>(
    <span class="hljs-string">`SELECT pr.display_name, SUM(p.quantity) AS total
     FROM purchases p
     JOIN products pr ON p.product_id = pr.id
     WHERE DATE_FORMAT(p.created_at, '%Y-%m') = ?
     GROUP BY p.product_id`</span>,
    [month],
    </span><span><span class="hljs-function">(<span class="hljs-params">err, purchases</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">if</span></span><span> (err) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>(err);

      report.</span><span><span class="hljs-property">purchases</span></span><span> = purchases;

      </span><span><span class="hljs-comment">// Monthly sales</span></span><span>
      db.</span><span><span class="hljs-title function_">query</span></span><span>(
        <span class="hljs-string">`SELECT pr.display_name, SUM(s.quantity) AS total
         FROM sales s
         JOIN products pr ON s.product_id = pr.id
         WHERE DATE_FORMAT(s.created_at, '%Y-%m') = ?
         GROUP BY s.product_id`</span>,
        [month],
        </span><span><span class="hljs-function">(<span class="hljs-params">err2, sales</span></span></span><span>) =&gt; {
          </span><span><span class="hljs-keyword">if</span></span><span> (err2) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>(err2);

          report.</span><span><span class="hljs-property">sales</span></span><span> = sales;
          res.</span><span><span class="hljs-title function_">json</span></span><span>(report);
        }
      );
    }
  );
});
</span></span></code></div></div></pre>

      </span><span><span class="hljs-title function_">runSales</span></span><span>();
    });
  } </span><span><span class="hljs-keyword">else</span></span><span> {
    result.</span><span><span class="hljs-property">purchases</span></span><span> = [];
    </span><span><span class="hljs-title function_">runSales</span></span><span>();
  }
});

</span><span><span class="hljs-comment">// ---------- MONTHLY REPORT (DETAILED) ----------</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">"/report/monthly"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {

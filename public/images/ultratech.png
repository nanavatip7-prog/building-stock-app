      </span><span><span class="hljs-keyword">if</span></span><span> (err) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>(err);

      </span><span><span class="hljs-keyword">if</span></span><span> (result.</span><span><span class="hljs-property">affectedRows</span></span><span> === </span><span><span class="hljs-number">0</span></span><span>) {
        </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">400</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">"   "</span></span><span> });

  </span><span><span class="hljs-keyword">const</span></span><span> products = </span><span><span class="hljs-keyword">await</span></span><span> res.</span><span><span class="hljs-title function_">json</span></span><span>();

  </span><span><span class="hljs-keyword">const</span></span><span> table = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"productTable"</span></span><span>);

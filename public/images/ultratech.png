</span><span><span class="hljs-comment">// ---------------- DAILY REPORT ----------------</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">"/report/daily"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {

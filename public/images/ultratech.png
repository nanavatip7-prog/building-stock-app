</span><span><span class="hljs-comment">// ---------- REPORT HELPERS ----------</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">buildFilter</span></span><span>(</span><span><span class="hljs-params">product_id</span></span><span>) {
  </span><span><span class="hljs-keyword">return</span></span><span> product_id ? </span><span><span class="hljs-string">" AND pr.id = ?"</span></span><span> : </span><span><span class="hljs-string">""</span></span><span>;
}

</span><span><span class="hljs-comment">// ---------- DAILY REPORT (DETAILED) ----------</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">"/report/daily"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {

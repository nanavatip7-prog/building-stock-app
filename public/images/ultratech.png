</span><span><span class="hljs-comment">// Purchase</span></span><span>
app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">"/purchase"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { product_id, quantity, dealer_name } = req.</span><span><span class="hljs-property">body</span></span><span>;
  </span><span><span class="hljs-keyword">if</span></span><span> (!product_id || !quantity || !dealer_name)
    </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">400</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">"   "</span></span><span> });

  db.</span><span><span class="hljs-title function_">query</span></span><span>(
    </span><span><span class="hljs-string">"UPDATE products SET stock = stock + ? WHERE id = ?"</span></span><span>,

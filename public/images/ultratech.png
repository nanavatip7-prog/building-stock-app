      result.</span><span><span class="hljs-property">sales</span></span><span> = rows;
      res.</span><span><span class="hljs-title function_">json</span></span><span>(result);
    });
  };

  </span><span><span class="hljs-keyword">if</span></span><span> (type !== </span><span><span class="hljs-string">"sale"</span></span><span>) {
    db.</span><span><span class="hljs-title function_">query</span></span><span>(purchasesQuery, params, </span><span><span class="hljs-function">(<span class="hljs-params">err, rows</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">if</span></span><span> (err) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>(err);
      result.</span><span><span class="hljs-property">purchases</span></span><span> = rows;
      </span><span><span class="hljs-title function_">runSales</span></span><span>();
    });
  } </span><span><span class="hljs-keyword">else</span></span><span> {
    result.</span><span><span class="hljs-property">purchases</span></span><span> = [];
    </span><span><span class="hljs-title function_">runSales</span></span><span>();
  }
});

app.</span><span><span class="hljs-title function_">listen</span></span><span>(</span><span><span class="hljs-number">4000</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {

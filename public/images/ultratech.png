});

</span><span><span class="hljs-comment">// ---------------- SALE () ----------------</span></span><span>
app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">"/sale"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { product_id, quantity, customer_name } = req.</span><span><span class="hljs-property">body</span></span><span>;

